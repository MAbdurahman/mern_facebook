{
  "name": "mern_facebook",
  "version": "1.0.0",
  "description": "implementation of Facebook clone utilizing MERN stack",
  "main": "server.js",
  "repository": "https://github.com/MAbdurahman/mern_facebook.git",
  "author": "Mahdi Abdurrahman <mdbdrrhm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node backend/server.js",
    "server": "SET NODE_ENV=DEVELOPMENT& nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "mongodb",
    "express",
    "react",
    "node"
  ],
  "nodemonConfig": {
    "delay": "2000"
  },
  "devDependencies": {
    "concurrently": "^7.1.0"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "morgan": "^1.10.0"
  }
}
